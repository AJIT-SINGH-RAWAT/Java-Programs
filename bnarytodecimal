package lecture3;
import java.util.*;
public class bnarytodecimal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s =new Scanner(System.in);
		
		int N=s.nextInt();
		int decimal=0;
		for(int i=0;N>0;i++)
		{
			int r=N%10;
			N=N/10;
			int c=(int)Math.pow(2,i);
			decimal=decimal+r*c;
			
		}
      System.out.println(decimal);
	}

}
