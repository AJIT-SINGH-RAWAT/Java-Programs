package test;

import java.io.*;
import java.util.*;

public class Solution
{
	public static void main(String[] args) {
    int n;
    Scanner sc = new Scanner(System.in);
    System.out.print("Enter the number : ");
    n = sc.nextInt();
    if ((n > 0) && ((n & (n - 1)) == 0))
        System.out.println("The number can be expressed in the power of two");
    else
        System.out.println("The number cannot be expressed in the power of two");
	}
}
